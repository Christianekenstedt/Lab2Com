@model IEnumerable<Lab2Community.Models.View.RecieverViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Senders</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th></th>
    </tr>
    
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.ActionLink("Filter", "From", new { id = item.RecieverId})
            </td>
        </tr>
    }

</table>
